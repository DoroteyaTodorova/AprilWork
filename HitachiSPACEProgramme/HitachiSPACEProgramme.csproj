<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="HitachiSPACEProgramme\Languages\de.language.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>de.language.resx</DependentUpon>
    </Compile>
    <Compile Update="HitachiSPACEProgramme\Languages\en.language.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>en.language.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="HitachiSPACEProgramme\Languages\de.language.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>de.language.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="HitachiSPACEProgramme\Languages\en.language.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>en.language.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
